<?xml version='1.0' encoding='utf-8'?>
<widget id="com.qm_translator.qm" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>قاموس عربي انجليزي</name>
    <description>
        قاموس من غير انترنت اوفلاين مجاني
    </description>
    <author email=" abd.200930@gmail.com" href="http://cordova.io">
       Abdelhadi Mohamed
    </author>
    <icon src="logo.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <splash src="splashscreen.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <preference name="orientation" value="portrait" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-dbcopy" spec="git+https://github.com/an-rahulpandey/cordova-plugin-dbcopy.git" />
    <plugin name="cordova-sqlite-storage" spec="^2.1.0" />
    <plugin name="cordova-plugin-x-toast" spec="^2.6.0" />
    <plugin name="cordova-plugin-admobpro" spec="^2.29.27" />
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.2.1" />
    <engine name="android" spec="^6.3.0" />
</widget>
